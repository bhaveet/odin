{
  "name": "odin",
  "version": "1.0.0",
  "description": "core module for citymall",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --forceExit --coverage --passWithNoTests --detectOpenHandles",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon --delay 0 --watch build build/index.js\"",
    "start": "npm run dev && node build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhaveet/odin.git"
  },
  "keywords": [
    "core_module",
    "core",
    "module",
    "logger",
    "connector"
  ],
  "author": "Bhaveet Kumar <bhaveet@citymall.live>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bhaveet/odin/issues"
  },
  "homepage": "https://github.com/bhaveet/odin#readme",
  "dependencies": {
    "auto-bind": "^5.0.1",
    "jest": "^27.5.1",
    "mongoose": "^6.2.4",
    "redis": "^4.0.4",
    "save": "^2.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/config": "^0.0.39",
    "@types/jest": "^27.4.0",
    "@types/kafkajs": "^1.9.0",
    "@types/node": "14.x",
    "@types/supertest": "^2.0.11",
    "concurrently": "^6.3.0",
    "gts": "^3.1.0",
    "husky": "^7.0.1",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.2",
    "pg-mem": "^2.1.7",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testMatch": [
      "**/build/**/*.(test|spec).js"
    ]
  }
}
